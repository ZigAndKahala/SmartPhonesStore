<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="rootPane" prefHeight="1080.0" prefWidth="1920.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.signInController">
   <children>
      <Label prefHeight="88.0" prefWidth="199.0" text="Sign In" AnchorPane.leftAnchor="20.0" AnchorPane.topAnchor="20.0">
         <font>
            <Font size="60.0" />
         </font>
      </Label>
      <Label layoutX="113.0" layoutY="221.0" text="User Id or Email Address" AnchorPane.leftAnchor="150.0" AnchorPane.topAnchor="200.0">
         <font>
            <Font size="40.0" />
         </font>
      </Label>
      <Label layoutX="116.0" layoutY="377.0" text="Password" AnchorPane.leftAnchor="150.0" AnchorPane.topAnchor="300.0">
         <font>
            <Font size="40.0" />
         </font>
      </Label>
      <TextField fx:id="userId" layoutX="410.0" layoutY="347.0" prefHeight="56.0" prefWidth="827.0" AnchorPane.leftAnchor="600.0" AnchorPane.topAnchor="200.0">
         <font>
            <Font size="24.0" />
         </font></TextField>
      <PasswordField fx:id="password" layoutX="297.0" layoutY="228.0" prefHeight="56.0" prefWidth="827.0" AnchorPane.leftAnchor="600.0" AnchorPane.topAnchor="300.0">
         <font>
            <Font size="24.0" />
         </font></PasswordField>
      <Button fx:id="signIn" mnemonicParsing="false" onAction="#signIn" text="Sign In" AnchorPane.bottomAnchor="20.0" AnchorPane.rightAnchor="40.0">
         <font>
            <Font size="40.0" />
         </font>
      </Button>
      <Button mnemonicParsing="false" onAction="#signUp" text="Sign Up" AnchorPane.bottomAnchor="20.0" AnchorPane.rightAnchor="230.0">
         <font>
            <Font size="40.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
